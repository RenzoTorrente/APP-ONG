{
  "name": "ong-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run copy && npm run build && cross-env NODE_ENV=production npm run serve",
    "build": "tsc",
    "dev": "npm run copy && cross-env NODE_ENV=development npm run serve",
    "copy": "cp -r views/ dist/ && cp -r public/ dist/ ",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "seed": "npx sequelize-cli db:seed:all",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.5",
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.960.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.1",
    "http-errors": "~1.6.3",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "mysql2": "^2.2.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.15",
    "@types/babel__generator": "^7.6.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.3",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsc-watch": "^1.0.13",
    "tslint": "^5.20.1",
    "typescript": "^4.3.5"
  }
}
